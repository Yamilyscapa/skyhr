{
  "cli": {
    "version": ">= 3.0.0",
    "appVersionSource": "local"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "channel": "development"
    },
    "preview": {
      "developmentClient": false,
      "distribution": "internal",
      "channel": "preview"
    },
    "production": {
      "developmentClient": false,
      "env": {
        "NODE_ENV": "production",
        "EXPO_PUBLIC_ENV": "production",
        "EXPO_NO_DOTENV": "1"
      },
      "ios": {
        "distribution": "store",
        "buildConfiguration": "Release"
      },
      "android": {
        "distribution": "store"
      },
      "node": "24.1.0",
      "channel": "production"
    }
  },
  "submit": {
    "production": {
      "ios": {
        "ascAppId": "6757027727"
      }
    }
  }
}
